  </div>
  <div class="field">
    <%= f.label :releaseNotes %><br>
    <%= f.text_area :releaseNotes %>
  </div>
  <div class="field">
    <%= f.label :platform %><br>
    <%= f.text_field :platform %>
  </div>
  <div class="field">
    <%= f.label :buildIdenifier %><br>
    <%= f.text_field :buildIdenifier %>
  </div>
  <div class="field">
    <%= f.label :accessToken %><br>
    <%= f.text_field :accessToken %>
  </div>
  <div class="field">
    <%= f.label :domain %><br>
    <%= f.text_field :domain %>
  </div>
  <div class="field">
    <%= f.label :provision %><br>
    <%= f.text_field :provision %>
  </div>
  <div class="field">
    <%= f.label :createDate %><br>
    <%= f.datetime_select :createDate %>
